{
  "expo": {
    "name": "ReactNative Demo",
    "slug": "AwesomeProject",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.zuhlke.learning.react-native",
      "supportsTablet": true
    },
    "android": {
      "package": "com.zuhlke.learning.react-native"
    }
  }
}
